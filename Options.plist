<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>name</key>
		    <string>Usertheme</string>
		<key>type</key>
		    <string>select</string>
		<key>label</key>
		    <string>Theme</string>
		<key>default</key>
		    <string>dark</string>
		<key>options</key>
            <dict>
                <key>Dark</key>
                <string>dark</string>
                <key>Light</key>
                <string>light</string>
            </dict>
	</dict>

		<dict>
		<key>name</key>
		    <string>Userscale</string>
		<key>type</key>
		    <string>select</string>
		<key>label</key>
		    <string>Scale</string>
		<key>default</key>
		    <string>100%</string>
		<key>options</key>
            <dict>
								<key>100%</key>
                <string>1.0</string>

								<key>75%</key>
                <string>0.75</string>

								<key>70%</key>
                <string>0.70</string>

								<key>60%</key>
                <string>0.60</string>

								<key>50%</key>
                <string>0.5</string>

            </dict>
	</dict>

	<dict>
		<key>name</key>
		    <string>UserfaceText</string>
		<key>type</key>
		    <string>edit</string>
		<key>label</key>
		    <string>Face Text</string>
		<key>default</key>
		    <string></string>
	</dict>

		<dict>
		<key>name</key>
		    <string>UseraccentColor</string>
		<key>type</key>
		    <string>edit</string>
		<key>label</key>
		    <string>Accent Color</string>
		<key>default</key>
		    <string>red</string>
	</dict>
</array>
</plist>
